{
  "name": "react-typescript-vite-preset",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --open",
    "start:dev": "vite --mode dev",
    "build:dev": "tsc && vite build --mode dev",
    "build:stage": "tsc && vite build --mode stage",
    "watch": "vite build --mode dev --watch",
    "scss-types": "npx typed-scss-modules src",
    "preview": "vite preview",
    "lint": "eslint ./src/"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "sass": "^1.58.0",
    "typed-scss-modules": "^7.0.2",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
